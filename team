<script>
    // Set image array
    images = [
        "aguilaair.jpg","ajabnoor.jpg","alex-barulescu.jpg","alpha-logician.jpg","batfink-007.jpg","ben-barrett.jpg","bogdan.jpg","captain-crypto.jpg","ch4ot1c.jpg","christophe-crespin.jpg","crazy-rich-btcp.jpg","cryptojay.jpg","dan-farina.jpg","david-akers.jpg","didi-taihuttu.jpg","eric.jpg","give-the-world-a-hope.jpg","j62.jpg","jazz-t.jpg","jay-pujanes.jpg","jisvy.jpg","jorge-diaz-romeral-gaminde.jpg","justin.jpg","liam.jpg","littlefoot.jpg","luis-fernandez.jpg","manuel-ascoli.jpg","matt-pass.jpg","max-tedi-cat.jpg","michael.jpg","mohsen-yeganeh.jpg","mr-btcp.jpg","mr-wh0.jpg","nuno_sousa-uva.jpg","open-mind.jpg","rain-maker.jpg","raspal.jpg","rebecca-telegram-bot.jpg","rod-mcqueen.jpg","roman.jpg","sam-hyde.jpg","sergio.jpg","snipple-rifksy.jpg","stefan-brodin.jpg","thiago-silva.jpg","tommy-moustache.jpg","truc-tran.jpg","tychon-borghois.jpg"
    ];
    // Put first 28 in faces1
    faces1 = '';
    for (var i=0; i<28; i++) {
        faces1 += '<img src="/uploads/images/team/' + images[i] + '" style="opacity: 0.4">';
    }
    // Put 28+ and 8 more into faces2
    faces2 = '';
    for (var i=28; i<images.length; i++) {
        faces2 += '<img src="/uploads/images/team/' + images[i] + '" style="opacity: 0.4">';
    }
    for (var i=0; i<8; i++) {
        faces2 += '<img src="/uploads/images/team/' + images[i] + '" style="opacity: 0.4">';
    }
    // Set faces 1-5 (1=3=5 and 2=4)
    document.getElementById("faces1").innerHTML = document.getElementById("faces3").innerHTML = document.getElementById("faces5").innerHTML = faces1;
    document.getElementById("faces2").innerHTML = document.getElementById("faces4").innerHTML = faces2;

    // Starting x position and opacity
    xPos = 0;
    opacity = 0;

    // Move and fade in on an interval
    var playAnim = function() {
        setInterval(function() {
            if (opacity < 1) {
                opacity += 0.02;
                document.getElementById('facesContainer').style.opacity = opacity;
            }
            xPos--;
            // Loop
            if (xPos <= -1400) {
                xPos = 0;
            }
            document.getElementById('faces').style.left = xPos + "px";
        },30);
    }
    // Start on page load
    window.addEventListener("load", function(){
        playAnim();
    });
</script>  	</div>
